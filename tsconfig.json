{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"]
		},
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "lib": [
      "ESNext"
    ],
    "types": [
      "@cloudflare/workers-types/2023-07-01",
      "vite/client"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx"
  },
}
